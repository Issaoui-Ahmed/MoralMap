{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "nextjs",
  "main": ".open-next/worker.js",
  "compatibility_date": "2025-03-01",
  "compatibility_flags": [
    "nodejs_compat"
    ],
  "assets": {
    "binding": "ASSETS",
    "directory": ".open-next/assets"
  },
  "observability": {
    "enabled": true
  },

   "kv_namespaces": [
    { "binding": "ROUTE_CONFIG_KV", "id": "00000000000000000000000000000000" },
    { "binding": "SESSION_DATA_KV", "id": "00000000000000000000000000000000" },
    { "binding": "USER_DATA_KV", "id": "00000000000000000000000000000000" }
  ]
}
